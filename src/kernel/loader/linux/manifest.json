{
    "$schema": "https://schemas.cute.engineering/stable/cutekit.manifest.component.v1",
    "type": "exe",
    "id": "kernel-loader-linux",
    "description": "kernel bootloader interface layer for limine ",
    "provides": [
        "kernel-entry"
    ],
    "requires": [
        "kernel",
        "libcore",
        "arch-x86-64",
        "arch-x86", 
        "hw-mem"
    ],
    "enableIf": {
        "arch": ["x86_64"],
        "sys": ["linux"]
    }
}